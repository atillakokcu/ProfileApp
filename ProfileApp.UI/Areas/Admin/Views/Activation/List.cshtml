@model List<TblActivationListDto>
@{
    ViewData["Title"] = "List";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<head>
<link href="~/admin/userlist/css/bootstrap1.min.css" rel="stylesheet" />
<link href="~/admin/userlist/vendors/themefy_icon/themify-icons.css" rel="stylesheet" />
<link href="~/admin/userlist/vendors/niceselect/css/nice-select.css" rel="stylesheet" />
<link rel="stylesheet" href="~/admin/userlist/vendors/owl_carousel/css/owl.carousel.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/gijgo/gijgo.min.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/font_awesome/css/all.min.css">
<link rel="stylesheet" href="~/admin/userlist/vendors/tagsinput/tagsinput.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/datepicker/date-picker.css">
<link rel="stylesheet" href="~/admin/userlist/vendors/vectormap-home/vectormap-2.0.2.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/scroll/scrollable.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/datatable/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="~/admin/userlist/vendors/datatable/css/responsive.dataTables.min.css">
<link rel="stylesheet" href="~/admin/userlist/vendors/datatable/css/buttons.dataTables.min.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/text_editor/summernote-bs4.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/morris/morris.css">

<link rel="stylesheet" href="~/admin/userlist/vendors/material_icon/material-icons.css">

<link rel="stylesheet" href="~/admin/userlist/css/metisMenu.css">

<link rel="stylesheet" href="~/admin/userlist/css/style1.css">
<link rel="stylesheet" href="~/admin/userlist/css/colors/default.css" id="colorSkinCSS">

</head>
<body>

<h1>User List</h1>
<div class="row">
    <div class="col-lg-12">
        <div class="white_card card_height_100 mb_30 pt-4">
            <div class="white_card_body">
                <div class="QA_section">
                    <div class="white_box_tittle list_header">
                        <h4>User List </h4>
                        <div class="box_right d-flex lms_block">
                            <div class="serach_field_2">
                                <div class="search_inner">
                                    <form active="#">
                                        <div class="search_field">
                                            <input type="text" placeholder="Search content here...">
                                        </div>
                                        <button type="submit"> <i class="ti-search"></i> </button>
                                    </form>
                                </div>
                            </div>
                            <div class="add_button ms-2">
                                <a href="#" data-toggle="modal" data-target="#addcategory" class="btn_1">search</a>
                            </div>
                        </div>
                    </div>
                    <div class="QA_table mb_30">

                        <table class="table lms_table_active ">
                            <thead>
                                <tr>
                                    <th scope="col">Number</th>
                                    <th scope="col">Activasyon Kodu</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Reserve-Delete-Add</th>
                                    <th><a asp-action="ActivaitonAdd" asp-controller="Activation" class="btn btn-primary rounded-pill mb-3">Activaiton Code Add</a> </th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                @{int sayac = 0;}
                                @foreach (var item in Model)
                                {
                                    sayac++;
                                    <tr>
                                        <th scope="row"> <a href="#" class="question_content"> @sayac </a></th>
                                        <td>@item.ActivationCode</td>
                                        <td>
                                            @if (@item.StatusId == 1)
                                            {
                                                <a href="#" class="btn btn-success mb-3">@item.StatusId</a>
                                            }
                                            else if (@item.StatusId == 2)
                                            {
                                                <a href="#" class="btn btn-danger mb-3">@item.StatusId</a>
                                            }
                                            else if (@item.StatusId == 3)
                                            {
                                                <a href="#" class="btn btn-warning mb-3">@item.StatusId</a>
                                            }
                                        </td>
                                        <td>
                                            <div class="action_btns d-flex">
                                                <a asp-action="ReserveActivaitonCode" asp-controller="Activation" asp-route-Id="@item.Id" class="action_btn mr_10"> <i class="far fa-edit"></i> </a>
                                                <a href="#" class="action_btn"> <i class="fas fa-trash"></i>    </a>
                                                
                                            </div>
                                        </td>
                                    </tr>

                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script data-cfasync="false" src="../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
<script src="~/admin/userlist/js/jquery1-3.4.1.min.js"></script>

<script src="~/admin/userlist/js/popper1.min.js"></script>

<script src="~/admin/userlist/js/bootstrap1.min.js"></script>

<script src="~/admin/userlist/js/metisMenu.js"></script>

<script src="~/admin/userlist/vendors/count_up/jquery.waypoints.min.js"></script>

<script src="~/admin/userlist/vendors/chartlist/Chart.min.js"></script>

<script src="~/admin/userlist/vendors/count_up/jquery.counterup.min.js"></script>

<script src="~/admin/userlist/vendors/niceselect/js/jquery.nice-select.min.js"></script>

<script src="~/admin/userlist/vendors/owl_carousel/js/owl.carousel.min.js"></script>

<script src="~/admin/userlist/vendors/datatable/js/jquery.dataTables.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/dataTables.responsive.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/dataTables.buttons.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/buttons.flash.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/jszip.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/pdfmake.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/vfs_fonts.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/buttons.html5.min.js"></script>
<script src="~/admin/userlist/vendors/datatable/js/buttons.print.min.js"></script>

<script src="~/admin/userlist/vendors/datepicker/datepicker.js"></script>
<script src="~/admin/userlist/vendors/datepicker/datepicker.en.js"></script>
<script src="~/admin/userlist/vendors/datepicker/datepicker.custom.js"></script>
<script src="~/admin/userlist/js/chart.min.js"></script>
<script src="~/admin/userlist/vendors/chartjs/roundedBar.min.js"></script>

