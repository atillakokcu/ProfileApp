@model TblActivationCreateDto
@{
    ViewData["Title"] = "ActivaitonAdd";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <div class="col-md-9">
        <div class="tab-content">
            <div class="tab-pane fade active show" id="account-social-links">
                <div class="card-body pb-2 ">



                    <div class="form-group">
                        <input type="text" class="form-control" asp-for="@Model.ActivationCode" placeholder="Activation code: s54ST8VpR1">
                        <input type="text" class="form-control" asp-for="@Model.StatusId" placeholder=" Status Id 1-2-3">
                    </div>
                </div>
            </div>
        </div>

        <div class="text-right mt-3">
            <button type="submit" asp-action="ActivaitonAdd" asp-controller="Activation" class="btn btn-primary">Add Activaiton Code</button>&nbsp;
        </div>
    </div>
</form>


